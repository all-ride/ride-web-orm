{
    "routes": [
        {
            "path": "/admin/system/orm",
            "controller": "ride\\web\\orm\\controller\\BuilderController",
            "action": "indexAction",
            "id": "system.orm"
        },
        {
            "path": "/admin/system/orm/model/%model%",
            "controller": "ride\\web\\orm\\controller\\BuilderController",
            "action": "modelAction",
            "id": "system.orm.model"
        },
        {
            "path": "/admin/system/orm/model/%model%/scaffold",
            "controller": "ride\\web\\orm\\controller\\BuilderController",
            "action": "scaffoldAction",
            "id": "system.orm.scaffold"
        },
        {
            "path": "/admin/system/orm/model/%model%/scaffold/%locale%",
            "controller": "ride\\web\\orm\\controller\\BuilderController",
            "action": "scaffoldAction",
            "id": "system.orm.scaffold.index"
        },
        {
            "path": "/admin/system/orm/model/%model%/scaffold/%locale%/%action%",
            "controller": "ride\\web\\orm\\controller\\BuilderController",
            "action": "scaffoldAction",
            "id": "system.orm.scaffold.action"
        },
        {
            "path": "/admin/system/orm/model/%model%/scaffold/%locale%/%id%/%action%",
            "controller": "ride\\web\\orm\\controller\\BuilderController",
            "action": "scaffoldAction",
            "id": "system.orm.scaffold.action.data"
        },
        {
            "path": "/admin/system/orm/model/%model%/scaffold/%locale%/export/%format%",
            "controller": "ride\\web\\orm\\controller\\BuilderController",
            "action": "scaffoldAction",
            "id": "system.orm.scaffold.export",
            "arguments": [
                {
                    "name": "action",
                    "type": "scalar",
                    "properties": {
                        "value": "export"
                    }
                }
            ]
        },
        {
            "path": "/api/v1/data/%model%/list",
            "controller": "ride\\web\\orm\\controller\\RestController",
            "action": "listAction",
            "id": "api.orm.list"
        }
    ]
}
